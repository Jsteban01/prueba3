{
  "builds": [{
    "src": "Mining/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Mining/wsgi.py"
    }
  ],

  "rewrites": [
    {"source": "/(.*)", "destination": "../servicios.html"}
  ]

}